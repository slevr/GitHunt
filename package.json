{
  "name": "githunt",
  "version": "1.0.0",
  "description": "Example app for Apollo",
  "main": "index.js",
  "scripts": {
    "setup": "cp ./config/.env.default ./.env && npm run migrate && npm run seed",
    "start-server": "nodemon api/index.js --watch api --exec babel-node",
    "start-client": "webpack-dev-server -d --hot --inline --no-info --port 3000",
    "start": "concurrently --kill-others \"npm run start-server\" \"npm run start-client\"",
    "test": "mocha --compilers js:babel-core/register --reporter spec --full-trace 'api/**/*.test.js'",
    "seed": "knex seed:run",
    "migrate": "knex migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollostack/GitHunt.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apollostack/GitHunt/issues"
  },
  "homepage": "https://github.com/apollostack/GitHunt#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.17.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "apollo-client": "^0.3.12",
    "apollo-server": "^0.1.1",
    "body-parser": "^1.15.1",
    "connect-session-knex": "^1.0.23",
    "cookie-parser": "^1.4.1",
    "dataloader": "^1.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "json-loader": "^0.5.4",
    "knex": "^0.11.5",
    "lodash": "^4.12.0",
    "node-emoji": "^1.3.0",
    "objection": "0.5.0-rc.5",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "react": "^15.0.2",
    "react-apollo": "^0.3.4",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.1",
    "react-timeago": "^3.0.0",
    "redux": "^3.5.2",
    "request-promise": "^3.0.0",
    "sqlite3": "^3.1.4"
  }
}
